{
    "manufacturer": "Gigabyte",
    "keyboard_name": "Gigabyte Force K83",
    "maintainer": "Alvin Meng",
    "bootloader": "halfkay",
    "diode_direction": "ROW2COL",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "nkro": true
    },
    "indicators": {
        "num_lock": "LINE_PIN32",
        "caps_lock": "LINE_PIN30"
    },
    "matrix_pins": {
        // In order: y0 -> y7
        "rows": [
            "LINE_PIN0",
            "LINE_PIN1",
            "LINE_PIN2",
            "LINE_PIN3",
            "LINE_PIN4",
            "LINE_PIN5",
            "LINE_PIN6",
            "LINE_PIN7"
        ],
        // In order: x0 -> x16
        // 13 (on-board LED) is skipped
        "cols": [
            "LINE_PIN8",
            "LINE_PIN9",
            "LINE_PIN10",
            "LINE_PIN11",
            "LINE_PIN12",
            "LINE_PIN14",
            "LINE_PIN15",
            "LINE_PIN16",
            "LINE_PIN17",
            "LINE_PIN18",
            "LINE_PIN19",
            "LINE_PIN20",
            "LINE_PIN21",
            "LINE_PIN22",
            "LINE_PIN23",
            "LINE_PIN24",
            "LINE_PIN25"
        ]
    },
    "processor": "cortex-m4",
    "board": "IC_TEENSY_4_1",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "layouts": {
        "LAYOUT_fullsize_ansi": {
            "layout": [
                // [row, col] or [y, x]
                {"matrix": [0, 0], "x": 0, "y": 0}, // esc
                {"matrix": [0, 1], "x": 2, "y": 0}, // F1
                {"matrix": [0, 2], "x": 3, "y": 0}, // F2
                {"matrix": [0, 3], "x": 4, "y": 0}, // F3
                {"matrix": [0, 4], "x": 5, "y": 0}, // F4
                {"matrix": [0, 5], "x": 6.5, "y": 0}, // F5
                {"matrix": [0, 6], "x": 7.5, "y": 0}, // F6
                {"matrix": [0, 7], "x": 8.5, "y": 0}, // F7
                {"matrix": [0, 10], "x": 9.5, "y": 0}, // F8
                {"matrix": [0, 9], "x": 11, "y": 0}, // F9
                {"matrix": [0, 11], "x": 12, "y": 0}, // F10
                {"matrix": [0, 12], "x": 13, "y": 0}, // F11
                {"matrix": [0, 13], "x": 14, "y": 0}, // F12
                {"matrix": [0, 14], "x": 15.25, "y": 0}, // print screen
                {"matrix": [0, 15], "x": 16.25, "y": 0}, // scroll lock
                {"matrix": [0, 8], "x": 17.25, "y": 0}, // pause
                {"matrix": [1, 0], "x": 0, "y": 1.25}, // `
                {"matrix": [1, 1], "x": 1, "y": 1.25}, // 1
                {"matrix": [1, 2], "x": 2, "y": 1.25}, // 2
                {"matrix": [1, 3], "x": 3, "y": 1.25}, // 3
                {"matrix": [1, 4], "x": 4, "y": 1.25}, // 4
                {"matrix": [1, 5], "x": 5, "y": 1.25}, // 5
                {"matrix": [1, 6], "x": 6, "y": 1.25}, // 6
                {"matrix": [1, 7], "x": 7, "y": 1.25}, // 7
                {"matrix": [1, 10], "x": 8, "y": 1.25}, // 8
                {"matrix": [1, 9], "x": 9, "y": 1.25}, // 9
                {"matrix": [1, 11], "x": 10, "y": 1.25}, // 10
                {"matrix": [1, 12], "x": 11, "y": 1.25}, // -
                {"matrix": [1, 13], "x": 12, "y": 1.25}, // =
                {"matrix": [1, 14], "x": 13, "y": 1.25, "w": 2}, // backspace
                {"matrix": [1, 15], "x": 15.25, "y": 1.25}, // insert
                {"matrix": [1, 8], "x": 16.25, "y": 1.25}, // home
                {"matrix": [1, 16], "x": 17.25, "y": 1.25}, // page up
                {"matrix": [6, 16], "x": 18.5, "y": 1.25}, // num lock
                {"matrix": [7, 16], "x": 19.5, "y": 1.25}, // pad /
                {"matrix": [7, 11], "x": 20.5, "y": 1.25}, // pad *
                {"matrix": [6, 11], "x": 21.5, "y": 1.25}, // pad -
                {"matrix": [2, 0], "x": 0, "y": 2.25, "w": 1.5}, // tab
                {"matrix": [2, 1], "x": 1.5, "y": 2.25}, // q
                {"matrix": [2, 2], "x": 2.5, "y": 2.25}, // w
                {"matrix": [2, 3], "x": 3.5, "y": 2.25}, // e
                {"matrix": [2, 4], "x": 4.5, "y": 2.25}, // r
                {"matrix": [2, 5], "x": 5.5, "y": 2.25}, // t
                {"matrix": [2, 6], "x": 6.5, "y": 2.25}, // y
                {"matrix": [2, 7], "x": 7.5, "y": 2.25}, // u
                {"matrix": [2, 10], "x": 8.5, "y": 2.25}, // i
                {"matrix": [2, 9], "x": 9.5, "y": 2.25}, // o
                {"matrix": [2, 11], "x": 10.5, "y": 2.25}, // p
                {"matrix": [2, 12], "x": 11.5, "y": 2.25}, // [
                {"matrix": [2, 13], "x": 12.5, "y": 2.25}, // ]
                {"matrix": [2, 14], "x": 13.5, "y": 2.25, "w": 1.5}, // \
                {"matrix": [2, 15], "x": 15.25, "y": 2.25}, // delete
                {"matrix": [2, 8], "x": 16.25, "y": 2.25}, // end
                {"matrix": [2, 16], "x": 17.25, "y": 2.25}, // page down
                {"matrix": [6, 15], "x": 18.5, "y": 2.25}, // pad 7
                {"matrix": [7, 15], "x": 19.5, "y": 2.25}, // pad 8
                {"matrix": [7, 7], "x": 20.5, "y": 2.25}, // pad 9
                {"matrix": [6, 7], "x": 21.5, "y": 2.25, "h": 2}, // pad +
                {"matrix": [3, 0], "x": 0, "y": 3.25, "w": 1.75}, // caps lock
                {"matrix": [3, 1], "x": 1.75, "y": 3.25}, // a
                {"matrix": [3, 2], "x": 2.75, "y": 3.25}, // s
                {"matrix": [3, 3], "x": 3.75, "y": 3.25}, // d
                {"matrix": [3, 4], "x": 4.75, "y": 3.25}, // f
                {"matrix": [3, 5], "x": 5.75, "y": 3.25}, // g
                {"matrix": [3, 6], "x": 6.75, "y": 3.25}, // h
                {"matrix": [3, 7], "x": 7.75, "y": 3.25}, // j
                {"matrix": [3, 10], "x": 8.75, "y": 3.25}, // k
                {"matrix": [3, 9], "x": 9.75, "y": 3.25}, // l
                {"matrix": [3, 11], "x": 10.75, "y": 3.25}, // ;
                {"matrix": [3, 12], "x": 11.75, "y": 3.25}, // '
                {"matrix": [3, 13], "x": 12.75, "y": 3.25, "w": 2.25}, // enter
                {"matrix": [6, 14], "x": 18.5, "y": 3.25}, // pad 4
                {"matrix": [7, 14], "x": 19.5, "y": 3.25}, // pad 5
                {"matrix": [7, 12], "x": 20.5, "y": 3.25}, // pad 6
                {"matrix": [4, 0], "x": 0, "y": 4.25, "w": 2.25}, // left shift
                {"matrix": [4, 1], "x": 2.25, "y": 4.25}, // z
                {"matrix": [4, 2], "x": 3.25, "y": 4.25}, // x
                {"matrix": [4, 3], "x": 4.25, "y": 4.25}, // c
                {"matrix": [4, 4], "x": 5.25, "y": 4.25}, // v
                {"matrix": [4, 5], "x": 6.25, "y": 4.25}, // b
                {"matrix": [4, 6], "x": 7.25, "y": 4.25}, // n
                {"matrix": [4, 7], "x": 8.25, "y": 4.25}, // m
                {"matrix": [4, 10], "x": 9.25, "y": 4.25}, // ,
                {"matrix": [4, 9], "x": 10.25, "y": 4.25}, // .
                {"matrix": [4, 11], "x": 11.25, "y": 4.25}, // /
                {"matrix": [4, 13], "x": 12.25, "y": 4.25, "w": 2.75}, // right shift
                {"matrix": [5, 14], "x": 16.25, "y": 4.25}, // up
                {"matrix": [6, 9], "x": 18.5, "y": 4.25}, // pad 1
                {"matrix": [7, 9], "x": 19.5, "y": 4.25}, // pad 2
                {"matrix": [6, 12], "x": 20.5, "y": 4.25}, // pad 3
                {"matrix": [7, 8], "x": 21.5, "y": 4.25, "h": 2}, // pad enter
                {"matrix": [5, 0], "x": 0, "y": 5.25, "w": 1.25}, // left control
                {"matrix": [5, 1], "x": 1.25, "y": 5.25, "w": 1.25}, // left super
                {"matrix": [5, 2], "x": 2.5, "y": 5.25, "w": 1.25}, // left alt
                {"matrix": [5, 4], "x": 3.75, "y": 5.25, "w": 6.25}, // space
                {"matrix": [5, 7], "x": 10, "y": 5.25, "w": 1.25}, // right alt
                {"matrix": [5, 10], "x": 11.25, "y": 5.25, "w": 1.25}, // right super
                {"matrix": [5, 9], "x": 12.5, "y": 5.25, "w": 1.25}, // menu
                {"matrix": [5, 13], "x": 13.75, "y": 5.25, "w": 1.25}, // right control
                {"matrix": [5, 15], "x": 15.25, "y": 5.25}, // left
                {"matrix": [5, 8], "x": 16.25, "y": 5.25}, // down
                {"matrix": [5, 16], "x": 17.25, "y": 5.25}, // right
                {"matrix": [6, 13], "x": 18.5, "y": 5.25, "w": 2}, // pad 0
                {"matrix": [7, 13], "x": 20.5, "y": 5.25} // pad .
            ]
        }
    }
}
